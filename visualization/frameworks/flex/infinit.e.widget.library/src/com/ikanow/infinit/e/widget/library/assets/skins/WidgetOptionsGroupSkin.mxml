<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
	xmlns:s="library://ns.adobe.com/flex/spark"
	alpha.disabled="0.5">

	<fx:Metadata>
		<![CDATA[ 
        [HostComponent("spark.components.SkinnableContainer")]
    ]]>
	</fx:Metadata>

	<s:states>
		<s:State name="normal" />
		<s:State name="disabled" />
	</s:states>

	<s:Rect id="background"
		bottom="0"
		left="0"
		right="0"
		top="0">
		<s:fill>
			<s:LinearGradient rotation="90">
				<s:entries>
					<s:GradientEntry color="0xF4F4F4" />
					<s:GradientEntry color="0xF4F4F4" />
				</s:entries>
			</s:LinearGradient>
		</s:fill>
		<s:filters>
			<s:DropShadowFilter 
				color="0xCCCCCC"
				alpha="1"
				angle="270"
				blurX="3"
				blurY="3"
				distance="3"
				inner="true" />
		</s:filters>
	</s:Rect>

	<s:Group id="contentGroup"
		minWidth="0"
		minHeight="0"
		bottom="0"
		left="0"
		right="0"
		top="0">
		<s:layout>
			<s:BasicLayout />
		</s:layout>
	</s:Group>

</s:Skin>
